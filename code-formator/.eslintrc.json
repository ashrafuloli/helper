{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react-hooks"],
  "rules": {
    "no-alert": "error",
    "no-console": 2,
    "no-unused-vars": 1,
    "no-unreachable": 0,
    "react-hooks/ruler-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}

// rules
// "key": "value -> off/0, warning/1, error/2"
